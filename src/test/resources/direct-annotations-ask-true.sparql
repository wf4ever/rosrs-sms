ASK { 
	?ann ro:annotatesAggregatedResource <http://example.org/ROs/ro1/a%20workflow.t2flow> ;
	       a ro:AggregatedAnnotation ;
	       ao:body ?graph .
	GRAPH ?graph {
		<http://example.org/ROs/ro1/a%20workflow.t2flow> dcterms:license "GPL" .
	}
}

